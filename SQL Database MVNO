-- Analysis

SELECT Reason, COUNT(*) AS CancellationCount
FROM CancellationReasons
GROUP BY Reason
ORDER BY CancellationCount DESC;

SELECT YEAR(Cancellation_Date) AS Year, MONTH(Cancellation_Date) AS Month, COUNT(*) AS CancellationCount
FROM CancellationReasons
GROUP BY Year, Month
ORDER BY Year, Month;

--
SELECT Reason FROM CancellationReasons;

-- Count the number of users per plan
SELECT p.Plan_Name, COUNT(u.User_ID) AS Number_of_Users
FROM Plans AS p
LEFT JOIN Users AS u ON p.Plan_ID = u.Plan_ID
GROUP BY p.Plan_Name;

-- Identify churned customers based on call and message activity
SELECT cr.Customer_ID
FROM (
  SELECT DISTINCT u.User_ID
  FROM Users u
  LEFT JOIN CallRecords c ON u.Cellphone_Number = c.Caller_Number
  LEFT JOIN MessageRecords m ON u.Cellphone_Number = m.Sender_Number
  WHERE c.Caller_Number IS NULL AND m.Sender_Number IS NULL
) AS cu
JOIN CancellationReasons cr ON cu.User_ID = cr.User_ID;

-- Count churned users by City and Post_Code
SELECT City, Post_Code, COUNT(*) AS Churn_Count
FROM Users
WHERE User_ID IN (
  SELECT DISTINCT u.User_ID
  FROM Users u
  LEFT JOIN CallRecords c ON u.Cellphone_Number = c.Caller_Number
  LEFT JOIN MessageRecords m ON u.Cellphone_Number = m.Sender_Number
  WHERE c.Caller_Number IS NULL AND m.Sender_Number IS NULL
)
GROUP BY City, Post_Code;
