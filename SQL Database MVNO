DROP DATABASE IF EXISTS MVNO;

CREATE DATABASE MVNO;

-- Use the Database
USE MVNO;

-- Users
CREATE TABLE Users (
    User_ID INTEGER PRIMARY KEY,
    First_name VARCHAR(100),
    Last_name VARCHAR(100),
    Country VARCHAR(100),
    City VARCHAR(100),
    Post_Code VARCHAR(20),
    Email VARCHAR(100),
    Join_Date DATE,
    Plan_ID INTEGER,
    Occupation VARCHAR(100),
    Age INTEGER,
    Cellphone_Number VARCHAR(15)
);

-- Plans
CREATE TABLE Plans (
    Plan_ID INTEGER PRIMARY KEY,
    Plan_Name VARCHAR(50),
    Data_Allowance INTEGER,
    Talk_Time INTEGER,
    Texts INTEGER,
    Price DECIMAL(10, 2)
);

-- Top-Up Transaction
CREATE TABLE TopUpTransactions (
    Transaction_ID INTEGER PRIMARY KEY,
    User_ID INTEGER,
    Amount DECIMAL(10, 2),
    Date DATE,
    FOREIGN KEY (User_ID) REFERENCES Users(User_ID)
);

-- Call Record
CREATE TABLE CallRecords (
    Call_ID INTEGER PRIMARY KEY,
    Caller_Number VARCHAR(15),
    Receiver_Number VARCHAR(15),
    Call_Duration INTEGER,
    Call_Date DATE
);

-- Message Record
CREATE TABLE MessageRecords (
    Message_ID INTEGER PRIMARY KEY,
    Sender_Number VARCHAR(15),
    Receiver_Number VARCHAR(15),
    Message_Date DATE
);

-- Network Tower
CREATE TABLE NetworkTowers (
    Tower_ID INTEGER PRIMARY KEY,
    Location VARCHAR(100),
    Capacity INTEGER,
    Status VARCHAR(20)
);

-- Cancellation Reasons
CREATE TABLE CancellationReasons (
    Reason_ID INTEGER PRIMARY KEY,
    Customer_ID INTEGER,  -- Foreign key to reference the customer who canceled
    Cancellation_Date DATE,  -- Date when the cancellation occurred
    Reason TEXT  -- Text field to describe the reason for canceling
);

-- My Data
CREATE TABLE MyData (
    User_ID INTEGER,
    Data_Usage INTEGER,
    Call_Duration INTEGER,
    Last_Activity_Date DATE,
    Data_Usage_Details VARCHAR(500),
    Call_History_Details VARCHAR(500),
    FOREIGN KEY (User_ID) REFERENCES Users(User_ID)
);
-- Customer Reviews
CREATE TABLE CustomerReviews (
    Review_ID INTEGER AUTO_INCREMENT PRIMARY KEY,
    User_ID INTEGER,  -- Foreign key to reference the user who wrote the review
    Product_ID INTEGER,  -- Foreign key to reference the product being reviewed
    Review_Text TEXT,  -- The text of the customer review
    Review_Date DATE,  -- Date when the review was written
    Rating INTEGER,  -- Numeric rating (e.g., on a scale of 1 to 5)
    FOREIGN KEY (User_ID) REFERENCES Users(User_ID)
);

-- Create a table to track user roaming locations
CREATE TABLE RoamingLocations (
    Location_ID SERIAL PRIMARY KEY,
    User_ID INTEGER,
    Country VARCHAR(100),
    City VARCHAR(100),
    Latitude DECIMAL(9, 6),
    Longitude DECIMAL(9, 6),
    Roaming_Start TIMESTAMP,
    Roaming_End TIMESTAMP
);

